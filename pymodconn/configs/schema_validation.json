{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "if_model_image",
        "if_model_summary",
        "if_seed",
        "seed",
        "model_type_prob",
        "loss_prob",
        "loss",
        "quantiles",
        "metrics",
        "optimizer",
        "Adam",
        "SGD",
        "save_models_dir",
        "batch_size",
        "n_past",
        "n_future",
        "known_past_features",
        "unknown_future_features",
        "known_future_features",
        "control_future_cells",
        "all_layers_neurons",
        "all_layers_dropout",
        "IF_SIMPLE_MODEL",
        "encoder",
        "decoder"
    ],
    "properties": {
        "if_model_image": {
            "type": "integer",
            "minimum": 1,
            "maximum": 1
        },
        "if_model_summary": {
            "type": "integer",
            "minimum": 0,
            "maximum": 0
        },
        "if_seed": {
            "type": "integer",
            "minimum": 0,
            "maximum": 0
        },
        "seed": {
            "type": "integer",
            "minimum": 500,
            "maximum": 500
        },
        "model_type_prob": {
            "type": "string"
        },
        "loss_prob": {
            "type": "string"
        },
        "loss": {
            "type": "string"
        },
        "quantiles": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "metrics": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "optimizer": {
            "type": "string"
        },
        "Adam": {
            "type": "object",
            "properties": {
                "lr": {
                    "type": "number"
                },
                "b1": {
                    "type": "number"
                },
                "b2": {
                    "type": "number"
                },
                "epsi": {
                    "type": "number"
                }
            }
        },
        "SGD": {
            "type": "object",
            "properties": {
                "lr": {
                    "type": "number"
                },
                "momentum": {
                    "type": "number"
                }
            }
        },
        "save_models_dir": {
            "type": "string"
        },
        "batch_size": {
            "type": "integer",
            "minimum": 32,
            "maximum": 32
        },
        "n_past": {
            "type": "integer",
            "minimum": 672,
            "maximum": 672
        },
        "n_future": {
            "type": "integer",
            "minimum": 96,
            "maximum": 96
        },
        "known_past_features": {
            "type": "integer",
            "minimum": 31,
            "maximum": 31
        },
        "unknown_future_features": {
            "type": "integer",
            "minimum": 1,
            "maximum": 1
        },
        "known_future_features": {
            "type": "integer",
            "minimum": 15,
            "maximum": 15
        },
        "control_future_cells": {
            "type": "integer",
            "minimum": 1,
            "maximum": 1
        },
        "all_layers_neurons": {
            "type": "integer",
            "minimum": 128,
            "maximum": 128
        },
        "all_layers_dropout": {
            "type": "number",
            "minimum": 0.4,
            "maximum": 0.4
        },
        "IF_SIMPLE_MODEL": {
            "type": "object",
            "properties": {
                "IF": {
                    "type": "integer"
                },
                "ALL_RNN_DEPTH": {
                    "type": "integer"
                },
                "ALL_RNN_TYPE": {
                    "type": "string"
                },
                "ALL_RNN_BIDIRECTIONAL": {
                    "type": "integer"
                },
                "ALL_MHA_HEAD": {
                    "type": "integer"
                },
                "ALL_MHA_DEPTH": {
                    "type": "integer"
                },
                "ALL_TCN_KERNEL_SIZE": {
                    "type": "integer"
                },
                "ALL_TCN_NB_STACKS": {
                    "type": "integer"
                },
                "ALL_TCN_DILATIONS": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "CIT_option": {
                    "type": "integer"
                },
                "CIT_depth": {
                    "type": "integer"
                },
                "IF_ALL_TCN": {
                    "type": "integer"
                },
                "IF_ALL_RNN": {
                    "type": "integer"
                },
                "IF_ALL_MHA": {
                    "type": "integer"
                },
                "IF_ALL_NONE_GLUADDNORM_ADDNORM": {
                    "type": "integer"
                },
                "IF_ALL_GRN": {
                    "type": "integer"
                }
            }
        },
        "encoder": {
            "type": "object",
            "properties": {
                "TCN_input": {
                    "type": "object",
                    "properties": {
                        "IF_TCN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_TCN": {
                            "type": "integer"
                        },
                        "kernel_size": {
                            "type": "integer"
                        },
                        "nb_stacks": {
                            "type": "integer"
                        },
                        "dilations": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            }
                        }
                    }
                },
                "RNN_block_input": {
                    "type": "object",
                    "properties": {
                        "IF_RNN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_block": {
                            "type": "integer"
                        },
                        "IF_GRN_block": {
                            "type": "integer"
                        },
                        "rnn_depth": {
                            "type": "integer"
                        },
                        "rnn_type": {
                            "type": "string"
                        },
                        "IF_birectionalRNN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_deep": {
                            "type": "integer"
                        }
                    }
                },
                "IF_POS_ENCODE": {
                    "type": "integer"
                },
                "self_MHA_block": {
                    "type": "object",
                    "properties": {
                        "MHA_depth": {
                            "type": "integer"
                        },
                        "IF_MHA": {
                            "type": "integer"
                        },
                        "IF_GRN_block": {
                            "type": "integer"
                        },
                        "MHA_head": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_deep": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "decoder": {
            "type": "object",
            "properties": {
                "TCN_input": {
                    "type": "object",
                    "properties": {
                        "IF_TCN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_TCN": {
                            "type": "integer"
                        },
                        "kernel_size": {
                            "type": "integer"
                        },
                        "nb_stacks": {
                            "type": "integer"
                        },
                        "dilations": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            }
                        }
                    }
                },
                "RNN_block_input": {
                    "type": "object",
                    "properties": {
                        "IF_RNN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_block": {
                            "type": "integer"
                        },
                        "IF_GRN_block": {
                            "type": "integer"
                        },
                        "rnn_depth": {
                            "type": "integer"
                        },
                        "rnn_type": {
                            "type": "string"
                        },
                        "IF_birectionalRNN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_deep": {
                            "type": "integer"
                        }
                    }
                },
                "CIT_option": {
                    "type": "integer"
                },
                "IF_NONE_GLUADDNORM_ADDNORM_CIT_1": {
                    "type": "integer"
                },
                "option_1_depth": {
                    "type": "integer"
                },
                "IF_NONE_GLUADDNORM_ADDNORM_CIT_2": {
                    "type": "integer"
                },
                "attn_type": {
                    "type": "integer"
                },
                "option_2_depth": {
                    "type": "integer"
                },
                "IF_POS_ENCODE": {
                    "type": "integer"
                },
                "IF_SELF_CROSS_MHA": {
                    "type": "integer"
                },
                "SELF_CROSS_MHA_depth": {
                    "type": "integer"
                },
                "self_MHA_block": {
                    "type": "object",
                    "properties": {
                        "IF_MHA": {
                            "type": "integer"
                        },
                        "IF_GRN_block": {
                            "type": "integer"
                        },
                        "MHA_head": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_deep": {
                            "type": "integer"
                        }
                    }
                },
                "cross_MHA_block": {
                    "type": "object",
                    "properties": {
                        "IF_MHA": {
                            "type": "integer"
                        },
                        "IF_GRN_block": {
                            "type": "integer"
                        },
                        "MHA_head": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_deep": {
                            "type": "integer"
                        }
                    }
                },
                "TCN_output": {
                    "type": "object",
                    "properties": {
                        "IF_TCN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_TCN": {
                            "type": "integer"
                        },
                        "kernel_size": {
                            "type": "integer"
                        },
                        "nb_stacks": {
                            "type": "integer"
                        },
                        "dilations": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            }
                        }
                    }
                },
                "RNN_block_output": {
                    "type": "object",
                    "properties": {
                        "IF_RNN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_block": {
                            "type": "integer"
                        },
                        "IF_GRN_block": {
                            "type": "integer"
                        },
                        "rnn_depth": {
                            "type": "integer"
                        },
                        "rnn_type": {
                            "type": "string"
                        },
                        "IF_birectionalRNN": {
                            "type": "integer"
                        },
                        "IF_NONE_GLUADDNORM_ADDNORM_deep": {
                            "type": "integer"
                        }
                    }
                },
                "MERGE_STATES_METHOD": {
                    "type": "integer"
                }
            }
        }
    }
}